<h1>LOGINする！！</h1>

<%= form_with(url: login_path, scope: :session, local: true) do |f| %>

   <div>
     <%= f.label :email, 'メールアドレス' %>
     <%= f.email_field :email %>
   </div>
   
   <div>
     <%= f.label :password, 'Password' %>
     <%= f.password_field :password %>
   </div>
  
    <%= f.submit 'ログインする' %>
    
<% end %>    